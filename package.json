  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sparkida/winston-datadog/issues"
  },
  "homepage": "https://github.com/sparkida/winston-datadog#readme",
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^2.11.8",
    "dotenv": "^2.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "winston": "^2.2.0"
  }
}
